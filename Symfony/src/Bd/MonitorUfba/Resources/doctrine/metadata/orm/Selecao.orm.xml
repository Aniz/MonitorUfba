<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Selecao" table="selecao">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nota" type="string" column="nota" length="45" nullable="true"/>
    <field name="idAluno" type="string" column="id_aluno" length="45" nullable="true"/>
    <field name="idProjeto" type="string" column="id_projeto" length="45" nullable="true"/>
    <field name="aprovado" type="string" column="aprovado" length="45" nullable="true"/>
    <field name="horarioAtendimento" type="string" column="horario_atendimento" length="45" nullable="true"/>
  </entity>
</doctrine-mapping>
