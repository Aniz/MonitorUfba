{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Selecao</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nota</th>
                <td>{{ entity.nota }}</td>
            </tr>
            <tr>
                <th>Idaluno</th>
                <td>{{ entity.idAluno }}</td>
            </tr>
            <tr>
                <th>Idprojeto</th>
                <td>{{ entity.idProjeto }}</td>
            </tr>
            <tr>
                <th>Aprovado</th>
                <td>{{ entity.aprovado }}</td>
            </tr>
            <tr>
                <th>Horarioatendimento</th>
                <td>{{ entity.horarioAtendimento }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('selecao') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('selecao_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
